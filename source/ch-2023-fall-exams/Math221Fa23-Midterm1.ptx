					</choice>
					<choice>
						<statement>
							<p>
								False
							</p>
						</statement>
					</choice>
				</choices>
		</task>
		<task>
			<statement>
				<p>
					If <m>f(3)=2</m> then <m>\displaystyle{\lim_{x\to3}f(x)=2}</m>.
				</p>
			</statement>
				<choices multiple-correct="no">
