			</statement>
				<choices multiple-correct="no">
					<choice>
						<statement>
							<p>
								<m>\{{\mathbf{u}}+{\mathbf{v}},{\mathbf{v}}+{\mathbf{w}}\}</m>
							</p>
						</statement>
					</choice>
					<choice>
						<statement>
							<p>
								<m>\{{\mathbf{u}},{\mathbf{u}}+{\mathbf{v}}+{\mathbf{w}},{\mathbf{v}}+{\mathbf{w}}\}</m>
							</p>
						</statement>
					</choice>
					<choice>
						<statement>
							<p>
								<m>\{{\mathbf{u}}+{\mathbf{v}},{\mathbf{v}}+{\mathbf{w}},{\mathbf{w}}+{\mathbf{u}},{\mathbf{v}}\}</m>
							</p>
						</statement>
					</choice>
					<choice>
						<statement>
							<p>
								<m>\{{\mathbf{u}}-{\mathbf{v}},{\mathbf{u}}+{\mathbf{v}},{\mathbf{w}}\}</m>
							</p>
						</statement>
					</choice>
					<choice>
						<statement>
